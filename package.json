{
  "name": "eslint-config-unjs",
  "version": "0.3.2",
  "description": "ESLint config for unjs projects",
  "repository": "unjs/eslint-config",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/eslint.config.d.mts",
      "import": "./dist/eslint.config.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "eslint-config-inspector",
    "lint": "eslint . && prettier -c src",
    "lint:fix": "eslint . --fix && prettier -w src",
    "prepack": "pnpm build",
    "release": "pnpm test && changelogen --release && npm publish && git push --follow-tags",
    "test": "pnpm lint",
    "typegen": "node ./scripts/typegen.mjs"
  },
  "dependencies": {
    "@eslint/js": "^9.5.0",
    "eslint-plugin-markdown": "^5.0.0",
    "eslint-plugin-unicorn": "^53.0.0",
    "globals": "^15.6.0",
    "typescript-eslint": "^7.14.1"
  },
  "devDependencies": {
    "@eslint/config-inspector": "^0.5.0",
    "@types/eslint": "^8.56.10",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^20.14.9",
    "automd": "^0.3.7",
    "changelogen": "^0.5.5",
    "eslint": "^9.5.0",
    "eslint-typegen": "^0.2.4",
    "jiti": "^1.21.6",
    "prettier": "^3.3.2",
    "typescript": "^5.5.2",
    "unbuild": "^2.0.0"
  },
  "peerDependencies": {
    "eslint": "*",
    "typescript": "*"
  },
  "packageManager": "pnpm@9.4.0"
}
