{
  "name": "eslint-config-unjs",
  "version": "0.4.1",
  "description": "ESLint config for unjs projects",
  "repository": "unjs/eslint-config",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/eslint.config.d.mts",
      "import": "./dist/eslint.config.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "eslint-config-inspector",
    "lint": "eslint --flag unstable_ts_config . && prettier -c src",
    "lint:fix": "eslint --flag unstable_ts_config . --fix && prettier -w src",
    "prepack": "pnpm build",
    "release": "pnpm test && changelogen --release && npm publish && git push --follow-tags",
    "test": "pnpm lint",
    "typegen": "node ./scripts/typegen.mjs"
  },
  "dependencies": {
    "@eslint/js": "^9.14.0",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-unicorn": "^55.0.0",
    "globals": "^15.11.0",
    "typescript-eslint": "^8.12.2"
  },
  "devDependencies": {
    "@eslint/config-inspector": "^0.5.6",
    "@types/eslint": "^9.6.1",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.8.6",
    "automd": "^0.3.12",
    "changelogen": "^0.5.7",
    "eslint": "^9.14.0",
    "eslint-typegen": "^0.3.2",
    "jiti": "^2.4.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3",
    "unbuild": "^2.0.0"
  },
  "peerDependencies": {
    "eslint": "*",
    "typescript": "*"
  },
  "packageManager": "pnpm@9.12.3"
}
