{
  "name": "eslint-config-unjs",
  "version": "0.4.2",
  "description": "ESLint config for unjs projects",
  "repository": "unjs/eslint-config",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/eslint.config.d.mts",
      "import": "./dist/eslint.config.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "eslint-config-inspector",
    "lint": "eslint --flag unstable_ts_config . && prettier -c src",
    "lint:fix": "eslint --flag unstable_ts_config . --fix && prettier -w src",
    "prepack": "pnpm build",
    "release": "pnpm test && changelogen --release && npm publish && git push --follow-tags",
    "test": "pnpm lint",
    "typegen": "node ./scripts/typegen.mjs"
  },
  "dependencies": {
    "@eslint/js": "^9.23.0",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-unicorn": "^56.0.1",
    "globals": "^15.15.0",
    "typescript-eslint": "^8.28.0"
  },
  "devDependencies": {
    "@eslint/config-inspector": "^0.7.1",
    "@types/eslint": "^9.6.1",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.13.13",
    "automd": "^0.4.0",
    "changelogen": "^0.6.1",
    "eslint": "^9.23.0",
    "eslint-typegen": "^2.2.0",
    "jiti": "^2.4.2",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2",
    "unbuild": "^2.0.0"
  },
  "peerDependencies": {
    "eslint": "*",
    "typescript": "*"
  },
  "packageManager": "pnpm@9.15.9"
}
