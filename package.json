{
  "name": "eslint-config-unjs",
  "version": "0.5.0",
  "description": "ESLint config for unjs projects",
  "repository": "unjs/eslint-config",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": "./dist/eslint.config.mjs"
  },
  "types": "dist/eslint.config.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "eslint-config-inspector",
    "lint": "eslint . && prettier -c src",
    "lint:fix": "eslint . --fix && prettier -w src",
    "prepack": "pnpm build",
    "release": "pnpm test && changelogen --release && npm publish && git push --follow-tags",
    "test": "pnpm lint",
    "typegen": "pnpm build"
  },
  "dependencies": {
    "@eslint/js": "^9.39.1",
    "@eslint/markdown": "^7.5.1",
    "eslint-plugin-unicorn": "^62.0.0",
    "globals": "^16.5.0",
    "typescript-eslint": "^8.49.0"
  },
  "devDependencies": {
    "@eslint/config-inspector": "^1.4.2",
    "@types/eslint": "^9.6.1",
    "@types/node": "^24.2.0",
    "automd": "^0.4.2",
    "changelogen": "^0.6.2",
    "eslint": "^9.39.1",
    "eslint-typegen": "^2.3.0",
    "jiti": "^2.6.1",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3",
    "unbuild": "^3.6.1"
  },
  "peerDependencies": {
    "eslint": "*",
    "typescript": "*"
  },
  "engines": {
    "node": "22"
  },
  "packageManager": "pnpm@10.25.0"
}
