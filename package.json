{
  "name": "eslint-config-unjs",
  "version": "0.5.0",
  "description": "ESLint config for unjs projects",
  "repository": "unjs/eslint-config",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": "./dist/eslint.config.mjs"
  },
  "types": "dist/eslint.config.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "eslint-config-inspector",
    "lint": "eslint --flag unstable_ts_config . && prettier -c src",
    "lint:fix": "eslint --flag unstable_ts_config . --fix && prettier -w src",
    "prepack": "pnpm build",
    "release": "pnpm test && changelogen --release && npm publish && git push --follow-tags",
    "test": "pnpm lint",
    "typegen": "node ./scripts/typegen.mjs"
  },
  "dependencies": {
    "@eslint/js": "^9.32.0",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-unicorn": "^59.0.1",
    "globals": "^16.3.0",
    "typescript-eslint": "^8.39.0"
  },
  "devDependencies": {
    "@eslint/config-inspector": "^1.1.0",
    "@types/eslint": "^9.6.1",
    "@types/node": "^24.2.0",
    "automd": "^0.4.0",
    "changelogen": "^0.6.2",
    "eslint": "^9.32.0",
    "eslint-typegen": "^2.3.0",
    "jiti": "^2.5.1",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "unbuild": "^3.6.0"
  },
  "peerDependencies": {
    "eslint": "*",
    "typescript": "*"
  },
  "packageManager": "pnpm@10.14.0"
}
